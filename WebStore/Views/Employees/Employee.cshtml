@model WebStore.Models.Employee
<!DOCTYPE html>
<html>
    <head>
        <title>Сотрудники</title>
    </head>
    <body>  
        <div style="float: left; width: 100%; margin: 10px 0">
            <h1 style="display: block; float: left; padding: 0; margin: 0 50px 0 0">
                <a href="~/home" style="padding:10px 10px 15px 10px; line-height: 20px; border: solid 1px gray; color: black; text-decoration: none">главная</a>
            </h1>
            <table> 
                <tbody>
                    <tr>
                        <td style="padding: 5px 15px 5px 0">ID: </td>
                        <td style="padding: 5px 0">@Model.Id</td>
                    </tr> 
                    <tr>
                        <td style="padding: 5px 15px 5px 0">Фамилия: </td>
                        <td style="padding: 5px 0">@Model.LastName</td>
                    </tr> 
                    <tr>
                        <td style="padding: 5px 15px 5px 0">Имя: </td>
                        <td style="padding: 5px 0">@Model.FirstName</td>
                    </tr> 
                    <tr>
                        <td style="padding: 5px 15px 5px 0">Отчество: </td>
                        <td style="padding: 5px 0">@Model.Patronymic</td>
                    </tr> 
                    <tr>
                        <td style="padding: 5px 15px 5px 0">Возраст: </td>
                        <td style="padding: 5px 0">@Model.Age</td>
                    </tr> 
                    <tr>
                        <td style="padding: 5px 15px 5px 0">Подразделение: </td>
                        <td style="padding: 5px 0">@Model.Department.Title</td>
                    </tr> 
                    <tr>
                        <td colspan=2 style="padding: 15px 5px">
                            <a href="~/home/employees"> << Список</a>
                        </td>
                    </tr> 
                </tbody>
            </table>
        </div>
    </body>
</html> 
@model IEnumerable<WebStore.Models.Employee>

<!DOCTYPE html>
<html>
    <head>
        <title>Employees</title>
    </head>
    <body>
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>LastName</th>
                    <th>FirstName</th>
                    <th>Patronymic</th>
                    <th>Age</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var employee in Model)
                {
                <tr>
                    <td>@employee.Id</td>
                    <td>@employee.LastName</td>
                    <td>@employee.FirstName</td>
                    <td>@employee.Patronymic</td>
                    <td>@employee.Age</td>
                </tr>
            }
            </tbody>
        </table>
        
        <a href="~/home">Главная</a>
    </body>
</html>